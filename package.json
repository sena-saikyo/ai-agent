{
  "name": "ai-agent",
  "version": "0.2.0",
  "private": true,
  "main": "electron/main.ts",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:renderer\" \"npm:dev:main\"",
    "dev:renderer": "vite --config renderer/vite.config.ts",
    "dev:main": "npm run build:main && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron dist-electron/main.js",
    "build:main": "tsc -p electron/tsconfig.json",
    "clean": "rimraf dist out",
    "build:renderer": "vite build --config renderer/vite.config.ts"
  },
  "dependencies": {
    "electron": "28.3.3",
    "firebase": "10.12.4",
    "googleapis": "131.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "22.7.4",
    "@types/react": "18.3.9",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.4",
    "concurrently": "9.1.2",
    "cross-env": "7.0.3",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "tsx": "^4.21.0",
    "typescript": "5.5.4",
    "vite": "5.4.10"
  }
}
